{
  "name": "nocms-validation",
  "version": "0.0.0-development",
  "description": "Validation rules for form",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "build": "babel ./src --out-dir ./lib --source-maps --presets es2015",
    "coverage": "tap --cov --coverage-report=lcov test/*.js",
    "prepare": "babel ./src --out-dir ./lib --source-maps --presets env",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "eslint": "eslint ."
  },
  "dependencies": {
    "norwegian-validation": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "condition-circle": "^1.5.0",
    "eslint": "^4.9.0",
    "eslint-config-webteam": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "semantic-release": "^8.2.0",
    "tap": "^5.8.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/miles-no/nocms-validation.git"
  },
  "release": {
    "verifyConditions": "condition-circle",
    "debug": false
  }
}
